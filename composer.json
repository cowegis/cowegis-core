{
  "name": "cowegis/cowegis-core",
  "description": "Cowegis Core - Content Web GIS Core",
  "license": "LGPL-3.0-or-later",
  "type": "library",
  "keywords": [
    "gis",
    "leaflet",
    "osm",
    "open street map",
    "webgis"
  ],
  "authors": [
    {
      "name": "David Molineus",
      "email": "david.molineus@netzmacht.de"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-json": "*",
    "ext-pdo": "*",
    "beberlei/assert": "^3.2",
    "cowegis/cowegis-geojson": "^1.0@dev",
    "goldspecdigital/oooas": "^2.8",
    "psr/container": "^1.0",
    "psr/event-dispatcher": "^1.0",
    "psr/http-message": "^1.0"
  },
  "require-dev": {
    "doctrine/coding-standard": "^9.0",
    "phpcq/runner-bootstrap": "^1.0@dev",
    "phpspec/phpspec": "^6.1"
  },
  "repositories": [
    {
      "type": "path",
      "url": "/home/netzmacht/Daten/Projekte/Entwicklung/vcs/cowegis/cowegis-geojson"
    }
  ],
  "autoload": {
    "psr-4": {
      "Cowegis\\Core\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "spec\\Cowegis\\Core\\": "spec/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
